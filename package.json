{
  "name": "scully-plugin-image-sharp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "cp-plugin": "cp -R plugin/ dist/scully-image/plugin/",
    "build": "ng build --prod && npm run cp-plugin",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "nodemon -w plugin -w projects --exec \"npm run build\"",
    "watch:plugin": "nodemon -w plugin --exec \"npm run cp-plugin\"",
    "scully": "scully --pjFirst",
    "scully:serve": "scully serve",
    "docs:build": "npm run ng build documentation",
    "docs:deploy": "npx gh-pages -d dist/static -r https://github.com/scully-image/scully-image.github.io.git -b master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@scullyio/init": "0.0.25",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@scullyio/scully": "latest",
    "@scullyio/ng-lib": "latest",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.1.1",
    "potrace": "2.1.5",
    "nodemon": "^2.0.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "scully-image": "^0.0.5"
  }
